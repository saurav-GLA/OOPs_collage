class Student {
    String name;
    int roll;

    void Student() {
        System.out.println("null");
    }
    void Student(String name){
        System.out.println(name);
    }
    
}

class Student {
    String name;
    int roll;

    void Student(char a) {
        System.out.println("null");
    }
    void Student(){
        System.out.println(name);
    }
    
}
